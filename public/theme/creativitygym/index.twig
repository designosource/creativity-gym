{% include '_header.twig' %}
<body class="homepage">

    <!-- FACEBOOK -->
    <div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=526289437445586";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>


    <div class="container">   
   {% include '_nav.twig' %}

  <div class="page col-lg-12">
    <h1>{{ page.title }}</h1>
   
   <img src="{{ page.image|image }}" class="img-responsive" />

    {% if page.teaser != "" %}<h2>{{ page.teaser }}</h2>{% endif %}

    <div class="fb-like" data-href="http://www.facebook.com/thecreativitygym" data-width="300" data-show-faces="true" data-send="true"></div>

    <div class="contentBody">{{ page.body }}</div>

    

    {% include '_mailchimp.twig' %}




    {% include '_footercontent.twig' %}

  </div>

  
    </div><!-- /.container -->

{% include '_footer.twig' %}

</body>
</html>
